{
  "name": "docks",
  "description": "Extensible system for parsing and generating documentation. It just freaking works!",
  "license": "Apache-2.0",
  "licenseStart": "2018",
  "scripts": {
    "docks": "node src/cli.js --outfile .verb.md",
    "docs": "yarn docks && verb",
    "lint": "eslint src test --cache --fix --quiet --format codeframe",
    "test": "nyc --require esm asia -R codeframe",
    "commit-dry": "git status --porcelain && git add -A && gitcommit -sS",
    "precommit": "lint-staged && yarn test",
    "commit": "yarn commit-dry"
  },
  "engines": {
    "node": "^8.9.0 || ^10.12.0"
  },
  "dependencies": {
    "@babel/parser": "^7.1.3",
    "doctrine": "^3.0.0",
    "esm": "^3.0.84",
    "fast-glob": "^2.2.2",
    "mkdirp": "^0.5.1",
    "mri": "^1.1.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "asia": "^0.19.7",
    "eslint": "^5.8.0",
    "eslint-config-esmc": "^1.0.12",
    "gitcommit": "^1.0.9",
    "husky": "^1.1.2",
    "lint-staged": "^8.0.4",
    "new-release": "^4.0.3",
    "nyc": "^13.0.0"
  },
  "files": [
    "src",
    "index.js",
    "cli.js"
  ],
  "bin": "cli.js",
  "main": "index.js",
  "module": "src/index.js",
  "typings": "src/index.d.ts",
  "version": "0.0.0-semantically-released",
  "repository": "tunnckoCore/docks",
  "homepage": "https://github.com/tunnckoCore/docks",
  "author": "Charlike Mike Reagent (https://tunnckocore.com)",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "eslintConfig": {
    "extends": "esmc"
  },
  "renovate": {
    "extends": "tunnckocore"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,es6,jsx,mjs}": [
      "eslint --cache --fix --quiet --format codeframe",
      "git add"
    ],
    "**/*.{js,ts,tsx,es6,jsx,mjs,md,mdx,json,yaml,yml,toml}": [
      "prettier --write",
      "git add"
    ]
  },
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 4
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "asia",
        "gitcommit",
        "new-release",
        "xaxa",
        "charlike"
      ]
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": [
      "new-release",
      "semantic-release"
    ]
  }
}
